tunnel: {{ .cloudflare.tunnel_id }}
credentials-file: /etc/cloudflared/credentials.json

ingress:
  - hostname: {{ .cloudflare.grafana_subdomain }}.{{ .cloudflare.domain }}
    service: http://grafana:3000
  - hostname: {{ .cloudflare.otel_subdomain }}.{{ .cloudflare.domain }}
    service: http://otel-collector:4318
  - service: http_status:404
